@model List<TravelTripProject.Models.Classes.Blog>

<div class="box1-left">

    @foreach (var item in Model)
    {
        <div class="bot-img">
            <img src="@item.BlogImage" alt="" />
        </div>
        <div class="bot-desc">
            <p>@item.Aciklama.Substring(0, 70)...</p>
        </div>
        <div class="clear"></div>
    }    
</div>
